<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--sequence-->
        <record id="seq_planing_client_product" model="ir.sequence">
            <field name="name">Planing Client Product</field>
            <field name="code">planing.client.product</field>
            <field name="prefix">/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_instance_client" model="ir.sequence">
            <field name="name">Instance Client</field>
            <field name="code">instance.client</field>
            <field name="prefix">/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_docker_build" model="ir.sequence">
            <field name="name">Docker Build</field>
            <field name="code">docker.build</field>
            <field name="prefix">/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="product_product_planing_pb" model="product.product">
            <field name="name">Service: Plan Basic</field>
            <field name="list_price">0.0</field>
            <field name="standard_price">1.0</field>
            <field name="type">service</field>
            <field name="supplier_taxes_id" eval="False"/>
            <field name="sale_ok" eval="True"/>
            <field name="recurring_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="default_code">PC-PB000001</field>
            <field name="invoice_policy">order</field>
        </record>
        <record id="product_product_planing_st" model="product.product">
            <field name="name">Service: Plan Standard</field>
            <field name="list_price">0.0</field>
            <field name="standard_price">1.0</field>
            <field name="type">service</field>
            <field name="supplier_taxes_id" eval="False"/>
            <field name="sale_ok" eval="True"/>
            <field name="recurring_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="default_code">PC-PS000001</field>
            <field name="invoice_policy">order</field>
        </record>
        <record id="product_product_planing_ad" model="product.product">
            <field name="name">Service: Plan Advance</field>
            <field name="list_price">0.0</field>
            <field name="standard_price">1.0</field>
            <field name="type">service</field>
            <field name="supplier_taxes_id" eval="False"/>
            <field name="sale_ok" eval="True"/>
            <field name="recurring_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="default_code">PC-PA000001</field>
            <field name="invoice_policy">order</field>
        </record>
        <record id="product_product_planing_pm" model="product.product">
            <field name="name">Service: Plan Premium</field>
            <field name="list_price">0.0</field>
            <field name="standard_price">1.0</field>
            <field name="type">service</field>
            <field name="supplier_taxes_id" eval="False"/>
            <field name="sale_ok" eval="True"/>
            <field name="recurring_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="default_code">PC-PM000001</field>
            <field name="invoice_policy">order</field>
        </record>

        <record id="type_planing_client_01" model="type.planing.client">
            <field name="type_planing">basic</field>
            <field name="count_user">1</field>
            <field name="product_id" ref="product_product_planing_pb"/>
        </record>
        <record id="type_planing_client_02" model="type.planing.client">
            <field name="type_planing">standard</field>
            <field name="count_user">3</field>
            <field name="product_id" ref="product_product_planing_st"/>
        </record>
        <record id="type_planing_client_03" model="type.planing.client">
            <field name="type_planing">advance</field>
            <field name="count_user">5</field>
            <field name="product_id" ref="product_product_planing_ad"/>
        </record>
        <record id="type_planing_client_04" model="type.planing.client">
            <field name="type_planing">premium</field>
            <field name="count_user">10</field>
            <field name="product_id" ref="product_product_planing_pm"/>
        </record>

        <record id="tag_planing_01" model="crm.tag">
            <field name="name">14.0</field>
            <field name="color">11</field>
        </record>
        <record id="tag_planing_02" model="crm.tag">
            <field name="name">13</field>
            <field name="color">4</field>
        </record>

        <record id="docker_image_00" model="docker.image">
            <field name="display_name">imtodooweb/odoo</field>
            <field name="tag_image" eval="[(6, 0, [ref('tag_planing_01')])]"/>
            <field name="size_image">1.4</field>
            <field name="type_image">od</field>
            <field name="image_id">c4370cddec27</field>
            <field name="status">not_used</field>
        </record>
        <record id="docker_image_01" model="docker.image">
            <field name="display_name">imtodooweb/postgres</field>
            <field name="tag_image" eval="[(6, 0, [ref('tag_planing_02')])]"/>
            <field name="size_image">314.68</field>
            <field name="type_image">po</field>
            <field name="image_id">293e4ed402ba</field>
            <field name="status">not_used</field>
        </record>
    </data>
</odoo>
